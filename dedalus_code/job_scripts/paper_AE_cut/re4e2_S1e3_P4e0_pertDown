#PBS -S /bin/bash
#PBS -N erf_step_3D_re4e2_P4e0_S_1e3_paper_AE_pertDown
#PBS -l select=7:ncpus=40:mpiprocs=40:model=sky_ele
#PBS -l walltime=12:00:00
#PBS -j oe
#PBS -q long
#PBS -W group_list=s2276

source /swbuild/eanders/miniconda3/etc/profile.d/conda.sh
conda activate dedalus
cd $PBS_O_WORKDIR

RE=4e2
PEN=4e0
STIFF=1e3
CONF=config/paper_AE_cut/erf_re1e3_P4e0_S1e3_pertDown
date
mpiexec_mpt -n 256 python3 erf_step_3D.py  $CONF  > out.paper_AE_pertDown.erf.re$RE.P$PEN.S$STIFF.$PBS_JOBID
date
