#PBS -W group_list=s2276

source /swbuild/eanders/miniconda3/etc/profile.d/conda.sh
conda activate dedalus
cd $PBS_O_WORKDIR

RE=1e3
PEN=1e0
STIFF=1e3
CONF=config/threeD_runs/erf_re1e3_P1e0_S1e3

date
mpiexec_mpt -n 1024 python3 erf_step_3D.py  $CONF  > out.3D.erf.schwarzschildIC.re$RE.P$PEN.S$STIFF.$PBS_JOBID
date
