source /swbuild/eanders/miniconda3/etc/profile.d/conda.sh
conda activate dedalus
cd $PBS_O_WORKDIR

RE=2e2
PEN=4e0
STIFF=1e3
CONF=config/paper_erf_Re_cut/pertUp_erf_re2e2_P4e0_S1e3

date
mpiexec_mpt -n 256 python3 erf_step_3D.py  $CONF  > out.paper_re_cut_pertUp.erf.re$RE.P$PEN.S$STIFF.$PBS_JOBID
date
